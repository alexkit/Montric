<div id="adminTabRightContent">
    <h1>{{id}}</h1>
    <table class="table">
        <tr>
            <td>Activated:</td>
            <td colspan="3">{{view Ember.Checkbox checkedBinding="alertActivated"}}</td>
        </tr>
        <tr>
            <td>Access Token:</td>
            <td>{{id}}</td>
        </tr>
        <tr>
            <td>Access Token Name:</td>
            <td>{{accessTokenName}}</td>
        </tr>
        <tr>
            <td>Account Name:</td>
            <td>{{accountName}}</td>
        </tr>
    </table>
</div>